{
  "printWidth": 80,
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "spaceBeforeFunctionParen": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "singleQuote": true,
        "parser": "json"
      }
    }
  ]
}
